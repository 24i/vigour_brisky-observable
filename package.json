{
  "name": "vigour-observable",
  "version": "3.1.2",
  "description": "fast and mem-efficient observables",
  "main": "lib/index.js",
  "scripts": {
    "test": "node test | tap-difflet && standard",
    "watch": "nodemon test | tap-difflet",
    "cover": "istanbul cover --report none --print detail test/index.js",
    "docs": "node_modules/vigour-doc/bin/vdoc",
    "travis": "npm run cover -s && istanbul report lcov && ((cat coverage/lcov.info | coveralls) || exit 0) && standard",
    "perf-browser": "budo ./test/performance/index.js -p 8080 --live"
  },
  "dependencies": {
    "vigour-base": "^3.3.9",
    "vigour-stamp": "^1.1.1",
    "vigour-util": "^3.0.0",
    "babelify": "^7.3.0",
    "babel-preset-es2015": "^6.6.0"
  },
  "devDependencies": {
    "tap-difflet": "0.4.0",
    "tape": "4.4.0",
    "observ": "0.2.0",
    "vigour-performance": "^1.2.3",
    "nodemon": "^1.9.1",
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.3",
    "vigour-doc": "^1.0.5",
    "standard": "^7.0.1",
    "pre-commit": "^1.1.3",
    "budo": "^8.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vigour-io/observable.git"
  },
  "keywords": [
    "observable",
    "mem",
    "vigour",
    "base"
  ],
  "author": "jim de beer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vigour-io/observable/issues"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "homepage": "https://github.com/vigour-io/observable#readme"
}
